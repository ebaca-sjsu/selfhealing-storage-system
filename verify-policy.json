{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": ["logs:CreateLogGroup", "logs:CreateLogStream", "logs:PutLogEvents"],
      "Effect": "Allow",
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Action": ["dynamodb:Scan", "dynamodb:UpdateItem"],
      "Effect": "Allow",
      "Resource": "arn:aws:dynamodb:us-west-2:289997608325:table/selfhealing-object-catalog"
    },
    {
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:PutObject",
        "s3:CopyObject",
        "s3:ListBucket"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::selfhealing-primary-dev",
        "arn:aws:s3:::selfhealing-primary-dev/*",
        "arn:aws:s3:::selfhealing-replica-dev",
        "arn:aws:s3:::selfhealing-replica-dev/*",
        "arn:aws:s3:::selfhealing-audit-dev",
        "arn:aws:s3:::selfhealing-audit-dev/*"
      ]
    }
  ]
}

